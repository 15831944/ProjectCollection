<local:SlidePresenter x:Class="DesktopClient.FlipPresenter"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:DesktopClient"
             mc:Ignorable="d" 
             d:DesignHeight="600" d:DesignWidth="800">
    
    <DockPanel>
        <Button x:Name="btnBack" Background="White" Content="&lt;" Width="50" Height="50" FontSize="20" DockPanel.Dock="Left" Click="btnBack_Click" />
        <Button x:Name="btnNext" Background="White" Content="&gt;" Width="50" Height="50" FontSize="20" DockPanel.Dock="Right" Click="btnNext_Click" />
        <ComboBox x:Name="cbbSlideNumber" Background="White" Width="200" DockPanel.Dock="Top" DropDownClosed="cbbSlideNumber_DropDownClosed" />
        <Grid x:Name="LayoutRoot" ClipToBounds="True">
            
        </Grid>
    </DockPanel>
</local:SlidePresenter>
