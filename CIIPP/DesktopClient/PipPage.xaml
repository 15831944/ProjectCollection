<Page x:Class="DesktopClient.PipPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:DesktopClient"
      mc:Ignorable="d" 
      d:DesignHeight="600" d:DesignWidth="800"
	Title="GroupSummaryPage" Loaded="Page_Loaded">

    <Grid>
        <DockPanel x:Name="LayoutRoot" Margin="0">
            <local:Banner x:Name="banner" Caption0="优先投资规划"  DockPanel.Dock="Top" />
            
            <local:PagerPresenter x:Name="Pager" PointsLocation="Top" Margin="20,0" SlideChanged="Pager_SlideChanged" />

        </DockPanel>
    </Grid>
</Page>
