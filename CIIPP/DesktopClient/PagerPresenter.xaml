<local:SlidePresenter x:Class="DesktopClient.PagerPresenter"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:DesktopClient"
             mc:Ignorable="d" 
             d:DesignHeight="600" d:DesignWidth="800">
    
    <DockPanel>
        <Border x:Name="NavBarContainer" DockPanel.Dock="Bottom">
            <local:NavPoints x:Name="NavBar" />
        </Border>
        <ScrollViewer x:Name="scroll1" VerticalScrollBarVisibility="Auto">
            <Grid x:Name="LayoutRoot" ClipToBounds="True" />
        </ScrollViewer>
    </DockPanel>
</local:SlidePresenter>
