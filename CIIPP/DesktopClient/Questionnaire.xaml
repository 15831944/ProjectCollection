      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      mc:Ignorable="d" 
      d:DesignHeight="1200" d:DesignWidth="800"
	Title="Questionnaire" Loaded="Page_Loaded">

    <StackPanel Background="DarkGray">
