<UserControl x:Class="Dreambuild.Gis.Desktop.PythonConsole"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300" Loaded="UserControl_Loaded">
    <DockPanel TextElement.FontFamily="Consolas">
        <TextBox x:Name="txtCmd" DockPanel.Dock="Bottom" Height="50" KeyDown="txtCmd_KeyDown" KeyUp="txtCmd_KeyUp" VerticalScrollBarVisibility="Auto" AcceptsReturn="True" />
        <TextBox x:Name="txtCmdHistory" TextWrapping="Wrap" IsReadOnly="True" VerticalScrollBarVisibility="Auto" />
    </DockPanel>
</UserControl>
