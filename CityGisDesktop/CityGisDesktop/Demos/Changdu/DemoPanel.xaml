<UserControl x:Class="Dreambuild.Gis.Desktop.Demos.Changdu.DemoPanel"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="260" Loaded="UserControl_Loaded_1">
    <Grid Margin="8">
        <ComboBox Name="SelectTopic" Margin="10,33,10,0" VerticalAlignment="Top" SelectionChanged="SelectTopic_SelectionChanged"/>
        <ComboBox Name="SelectItem" Margin="10,92,10,0" VerticalAlignment="Top" SelectionChanged="SelectItem_SelectionChanged" Visibility="Collapsed"/>
        <TextBlock HorizontalAlignment="Left" Margin="10,13,0,0" TextWrapping="Wrap" Text="专题" VerticalAlignment="Top"/>
        <TextBlock HorizontalAlignment="Left" Margin="10,72,0,0" TextWrapping="Wrap" Text="子项" VerticalAlignment="Top" Visibility="Collapsed"/>
        <StackPanel Name="LegendPanel" Margin="10,72,10,0" VerticalAlignment="Top"/>
    </Grid>
</UserControl>
